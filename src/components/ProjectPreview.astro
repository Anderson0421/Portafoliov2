---
    const {project} = Astro.props;
---
<div class="ml-10 flex flex-col gap-y-4">
    <img class="w-60" src={project.frontmatter.image.url} alt="xd">
    <a class="text-2xl" href={project.url}>{project.frontmatter.title}</a>
    <div class="pr-5">
        {project.frontmatter.description}
        {project.frontmatter.tags}
    </div>
</div>